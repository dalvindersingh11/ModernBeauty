{
    "name": "Modern_Beauty",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "clear": "cd android ./gradlew clean cd..",
        "ios": "react-native run-ios",
        "lint": "eslint .",
        "start": "react-native start",
        "test": "jest",
        "android:release": "cd android && ./gradlew app:assembleRelease",
        "android:clean": "cd android && ./gradlew clean && ./gradlew",
        "android:bundle": "cd android && ./gradlew bundleRelease",
        "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
        "pod": "cd ios && pod install && cd ..",
        "buildandroid": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
        "buildDebugApk": "cd android && ./gradlew clean && ./gradlew assembleDebug && cd ..",
        "reset": "npm start -- --reset-cache",
        "buildios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios"
    },
    "dependencies": {
        "@react-native-community/datetimepicker": "^8.4.1",
        "@react-navigation/native": "^7.1.9",
        "@react-navigation/stack": "^7.3.2",
        "react": "19.0.0",
        "react-native": "0.79.2",
        "react-native-country-codes-picker": "^2.3.5",
        "react-native-country-picker-modal": "^2.0.0",
        "react-native-element-dropdown": "^2.12.4",
        "react-native-gesture-handler": "^2.25.0",
        "react-native-modal-datetime-picker": "^18.0.0",
        "react-native-responsive-dimensions": "^3.1.1",
        "react-native-safe-area-context": "^5.4.1",
        "react-native-screens": "^4.10.0",
        "react-native-size-matters": "^0.4.2",
        "react-native-vector-icons": "^10.2.0",
        "react-native-video": "^6.14.0"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.3",
        "@babel/runtime": "^7.25.0",
        "@react-native-community/cli": "18.0.0",
        "@react-native-community/cli-platform-android": "18.0.0",
        "@react-native-community/cli-platform-ios": "18.0.0",
        "@react-native/babel-preset": "0.79.2",
        "@react-native/eslint-config": "0.79.2",
        "@react-native/metro-config": "0.79.2",
        "@react-native/typescript-config": "0.79.2",
        "@types/jest": "^29.5.13",
        "@types/react": "^19.0.0",
        "@types/react-native-vector-icons": "^6.4.18",
        "@types/react-test-renderer": "^19.0.0",
        "eslint": "^8.19.0",
        "jest": "^29.6.3",
        "prettier": "2.8.8",
        "react-test-renderer": "19.0.0",
        "typescript": "5.0.4"
    },
    "engines": {
        "node": ">=18"
    }
}
